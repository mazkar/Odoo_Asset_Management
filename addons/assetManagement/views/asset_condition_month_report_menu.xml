<odoo>
  <menuitem id="menu_condition_month_report_root"
            name="Inspection Reports"
            parent="asset_root_menu"
            sequence="50"/>

  <record id="action_condition_month_report" model="ir.actions.report">
    <field name="name">Monthly Inspection Report</field>
    <field name="model">x_asset.condition.month</field>
    <field name="report_type">qweb-pdf</field>
    <field name="report_name">assetManagement.report_condition_month</field>
    <field name="report_file">assetManagement.report_condition_month</field>
    <field name="print_report_name">'Inspection Report - %s' % (object.bulan_tahun)</field>
  </record>
</odoo>

